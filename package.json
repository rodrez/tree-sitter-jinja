{
  "name": "tree-sitter-jinja2",
  "version": "0.0.1",
  "description": "jinja2 grammar for tree-sitter",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "parsing",
    "incremental"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "node-gyp-build": "^4.8.0",
    "tree-sitter-html": "^0.20.2"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "prebuildify": "^6.0.0",
    "tree-sitter-cli": "0.22.0"
  },
  "tree-sitter": [
    {
      "scope": "source.text.jinja",
      "file-types": [
        "jinja"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "external-files": [
        "src/scanner.cc"
      ]
    }
  ],
  "scripts": {
    "install": "node-gyp-build",
    "build": "tree-sitter build",
    "lint": "eslint grammar.js",
    "test": "tree-sitter test && script/parse-examples",
    "test-windows": "tree-sitter test",
    "dev": "tree-sitter playground",
    "prebuildify": "prebuildify --napi --strip"
  }
}
